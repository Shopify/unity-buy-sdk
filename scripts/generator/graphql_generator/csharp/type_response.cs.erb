namespace <%= namespace %> {
    using System;
    using System.Collections.Generic;

    public class <%= type.classify_name %> : AbstractResponse {
        <%# output getters for fields %>
        <% type.fields.each do |field| %>
            public <%= response_type(field.type) %> <%= escape_reserved_word(field.name) %> {
                get {
                    return _<%= escape_reserved_word(field.name) %>;
                }
            }
        <% end %>
        
        <% type.fields.each do |field| %>
            private <%= response_type(field.type) %> _<%= escape_reserved_word(field.name) %>;
        <% end %>

        <%# Constructor %>
        public <%= type.classify_name %>(Dictionary<string, object> dataJSON) {
            DataJSON = dataJSON;
            
            <%= response_inits type %>
        }
    }
}
